{
  "expo": {
    "name": "app",
    "slug": "app",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "android": {
      "package": "com.bhavanishankar.bhavanishankarapp",
      "versionCode": 1
    },
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/appLogo.png",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/images/Splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#90c5e6"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },

    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "expo.ts",
        "expo.tsx",
        "expo.js",
        "expo.jsx",
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg"
      ]
    }
  }
}
